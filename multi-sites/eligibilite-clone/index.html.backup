    <!-- SCRIPTS -->
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
        // Initialize AOS
        AOS.init({
            duration: 800,
            easing: 'ease-out',
            once: true
        });

        // Form state
        let currentStep = 1;
        const totalSteps = 5;
        const formData = {
            project: '',
            housing_type: '',
            owner_status: '',
            construction_year: '',
            surface: '',
            heating_type: '',
            energy_bill: '',
            household_size: '',
            income: ''
        };

        // Project names for display
        const projectNames = {
            'pac': 'Pompe à chaleur',
            'isolation': 'Isolation',
            'solaire': 'Panneaux solaires',
            'chaudiere': 'Chaudière gaz',
            'vmc': 'VMC',
            'fenetres': 'Fenêtres / Portes',
            'multi': 'Plusieurs travaux'
        };

        const housingNames = {
            'maison': 'Maison',
            'appartement': 'Appartement'
        };

        const heatingNames = {
            'electrique': 'Électrique',
            'gaz': 'Gaz',
            'fioul': 'Fioul',
            'bois': 'Bois',
            'autre': 'Autre'
        };

        const incomeNames = {
            'tres_modeste': 'Très modeste',
            'modeste': 'Modeste',
            'intermediaire': 'Intermédiaire',
            'superieur': 'Supérieur'
        };

        // Select option
        function selectOption(card, field) {
            // Remove selected from siblings
            const parent = card.parentElement;
            parent.querySelectorAll('.option-card').forEach(c => c.classList.remove('selected'));

            // Add selected to clicked
            card.classList.add('selected');

            // Store value
            formData[field] = card.dataset.value;

            // Update estimation
            updateEstimation();
        }

        // Update estimation based on selections
        function updateEstimation() {
            let baseAmount = 5000;

            // Project type bonus
            if (formData.project === 'pac') baseAmount += 4000;
            if (formData.project === 'isolation') baseAmount += 2000;
            if (formData.project === 'solaire') baseAmount += 3000;
            if (formData.project === 'multi') baseAmount += 6000;

            // Income bonus
            if (formData.income === 'tres_modeste') baseAmount += 3000;
            if (formData.income === 'modeste') baseAmount += 2000;
            if (formData.income === 'intermediaire') baseAmount += 1000;

            // Surface bonus
            if (formData.surface === '100_150' || formData.surface === 'plus_150') baseAmount += 1000;

            const estimatedEl = document.getElementById('estimatedAid');
            if (estimatedEl) {
                estimatedEl.textContent = `jusqu'à ${baseAmount.toLocaleString('fr-FR')}€`;
            }
        }

        // Update progress bar
        function updateProgress() {
            const progressFill = document.getElementById('progressBarFill');
            const progress = ((currentStep - 1) / (totalSteps - 1)) * 100;
            progressFill.style.width = `calc(${progress}% - 60px)`;

            document.querySelectorAll('.progress-step').forEach((step, index) => {
                const stepNum = index + 1;
                step.classList.remove('active', 'completed');
                if (stepNum < currentStep) {
                    step.classList.add('completed');
                } else if (stepNum === currentStep) {
                    step.classList.add('active');
                }
            });
        }

        // Next step
        function nextStep() {
            if (currentStep < totalSteps) {
                // Validate current step
                if (!validateStep(currentStep)) {
                    return;
                }

                // Hide current
                document.querySelector(`.form-step[data-step="${currentStep}"]`).classList.remove('active');

                // Show next
                currentStep++;
                const nextStepEl = document.querySelector(`.form-step[data-step="${currentStep}"]`);
                nextStepEl.classList.add('active');

                // Update progress
                updateProgress();

                // Scroll to form
                document.getElementById('simulateur').scrollIntoView({ behavior: 'smooth', block: 'start' });
            }
        }

        // Previous step
        function prevStep() {
            if (currentStep > 1) {
                // Hide current
                document.querySelector(`.form-step[data-step="${currentStep}"]`).classList.remove('active');

                // Show previous
                currentStep--;
                document.querySelector(`.form-step[data-step="${currentStep}"]`).classList.add('active');

                // Update progress
                updateProgress();
            }
        }

        // Validate step
        function validateStep(step) {
            if (step === 1 && !formData.project) {
                alert('Veuillez sélectionner un type de travaux');
                return false;
            }
            if (step === 5) {
                const lastName = document.getElementById('lastName');
                const firstName = document.getElementById('firstName');
                const email = document.getElementById('email');
                const phone = document.getElementById('phone');
                const postalCode = document.getElementById('postalCode');
                const consent = document.getElementById('consent');

                let isValid = true;

                [lastName, firstName, email, phone, postalCode].forEach(input => {
                    input.classList.remove('error');
                });

                if (!lastName.value.trim()) {
                    lastName.classList.add('error');
                    isValid = false;
                }
                if (!firstName.value.trim()) {
                    firstName.classList.add('error');
                    isValid = false;
                }
                if (!email.value.trim() || !email.value.includes('@')) {
                    email.classList.add('error');
                    isValid = false;
                }
                if (!phone.value.trim() || phone.value.length < 10) {
                    phone.classList.add('error');
                    isValid = false;
                }
                if (!postalCode.value.trim() || postalCode.value.length !== 5) {
                    postalCode.classList.add('error');
                    isValid = false;
                }
                if (!consent.checked) {
                    alert('Veuillez accepter les conditions');
                    isValid = false;
                }

                return isValid;
            }
            return true;
        }

        // Submit form
        function submitForm() {
            if (!validateStep(5)) {
                return;
            }

            // Hide form content
            document.getElementById('formContent').style.display = 'none';
            document.querySelector('.progress-container').style.display = 'none';

            // Show success
            const successEl = document.getElementById('formSuccess');
            successEl.classList.add('active');

            // Update recap
            document.getElementById('recapProject').textContent = projectNames[formData.project] || '-';
            document.getElementById('recapHousing').textContent = housingNames[formData.housing_type] || '-';
            document.getElementById('recapHeating').textContent = heatingNames[formData.heating_type] || '-';
            document.getElementById('recapIncome').textContent = incomeNames[formData.income] || '-';
            document.getElementById('recapTotal').textContent = document.getElementById('estimatedAid')?.textContent || 'jusqu\'à 9 000€';

            // Launch confetti
            launchConfetti();

            // Scroll to top of form
            document.getElementById('simulateur').scrollIntoView({ behavior: 'smooth', block: 'start' });
        }

        // Launch confetti
        function launchConfetti() {
            const container = document.getElementById('confettiContainer');
            const colors = ['#F97316', '#2563EB', '#10B981', '#8B5CF6', '#EC4899', '#FBBF24'];

            for (let i = 0; i < 100; i++) {
                const confetti = document.createElement('div');
                confetti.className = 'confetti';
                confetti.style.left = Math.random() * 100 + '%';
                confetti.style.backgroundColor = colors[Math.floor(Math.random() * colors.length)];
                confetti.style.animationDelay = Math.random() * 2 + 's';
                confetti.style.transform = `rotate(${Math.random() * 360}deg)`;
                container.appendChild(confetti);

                // Remove after animation
                setTimeout(() => confetti.remove(), 5000);
            }
        }

        // Header scroll effect
        window.addEventListener('scroll', () => {
            const header = document.getElementById('header');
            if (window.scrollY > 100) {
                header.classList.add('scrolled');
            } else {
                header.classList.remove('scrolled');
            }
        });

        // Mobile menu
        document.getElementById('mobileMenuBtn').addEventListener('click', () => {
            document.getElementById('mobileMenu').classList.add('active');
        });

        document.getElementById('mobileMenuClose').addEventListener('click', () => {
            document.getElementById('mobileMenu').classList.remove('active');
        });

        function closeMobileMenu() {
            document.getElementById('mobileMenu').classList.remove('active');
        }

        // FAQ Accordion
        document.querySelectorAll('.faq-question').forEach(question => {
            question.addEventListener('click', () => {
                const item = question.parentElement;
                const isActive = item.classList.contains('active');

                document.querySelectorAll('.faq-item').forEach(i => i.classList.remove('active'));

                if (!isActive) {
                    item.classList.add('active');
                }
            });
        });

        // Counter Animation
        function animateCounter(counter, target) {
            const speed = 200;
            const increment = target / speed;
            let count = 0;

            const timer = setInterval(() => {
                count += increment;
                if (count >= target) {
                    counter.textContent = target.toLocaleString('fr-FR');
                    clearInterval(timer);
                } else {
                    counter.textContent = Math.ceil(count).toLocaleString('fr-FR');
                }
            }, 10);
        }

        // Animate avis counter on load
        setTimeout(() => {
            animateCounter(document.getElementById('avisCounter'), 126);
        }, 1500);

        // Intersection Observer for stat counters
        const observerOptions = { threshold: 0.5 };
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.querySelectorAll('.counter').forEach(counter => {
                        const target = parseInt(counter.dataset.target);
                        animateCounter(counter, target);
                    });
                    observer.unobserve(entry.target);
                }
            });
        }, observerOptions);

        document.querySelectorAll('.stats-grid').forEach(grid => {
            observer.observe(grid);
        });

        // Smooth scroll
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({ behavior: 'smooth', block: 'start' });
                }
            });
        });

        // Lazy loading images
        if ('IntersectionObserver' in window) {
            const imgObserver = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        const img = entry.target;
                        if (img.dataset.src) {
                            img.src = img.dataset.src;
                            img.classList.add('loaded');
                        }
                        imgObserver.unobserve(img);
                    }
                });
            });

            document.querySelectorAll('img[data-src]').forEach(img => {
                imgObserver.observe(img);
            });
        }

        // Touch swipe for form steps (mobile)
        let touchStartX = 0;
        let touchEndX = 0;

        const formContent = document.getElementById('formContent');

        formContent.addEventListener('touchstart', e => {
            touchStartX = e.changedTouches[0].screenX;
        }, { passive: true });

        formContent.addEventListener('touchend', e => {
            touchEndX = e.changedTouches[0].screenX;
            handleSwipe();
        }, { passive: true });

        function handleSwipe() {
            const swipeThreshold = 50;
            const diff = touchStartX - touchEndX;

            if (Math.abs(diff) > swipeThreshold) {
                if (diff > 0) {
                    // Swipe left - next step
                    nextStep();
                } else {
                    // Swipe right - previous step
                    prevStep();
                }
            }
        }

        // Initialize progress bar
        updateProgress();

        // Ripple effect on buttons
        function createRipple(event) {
            const button = event.currentTarget;
            const rect = button.getBoundingClientRect();
            const size = Math.max(rect.width, rect.height);
            const x = event.clientX - rect.left - size / 2;
            const y = event.clientY - rect.top - size / 2;

            const ripple = document.createElement('span');
            ripple.className = 'ripple';
            ripple.style.width = ripple.style.height = size + 'px';
            ripple.style.left = x + 'px';
            ripple.style.top = y + 'px';

            button.appendChild(ripple);

            setTimeout(() => ripple.remove(), 600);
        }

        // Add ripple to all buttons
        document.querySelectorAll('.btn-orange, .btn-next, .btn-submit, .btn-header').forEach(btn => {
            btn.classList.add('ripple-container');
            btn.addEventListener('click', createRipple);
        });

        // Add keyboard navigation for option cards
        document.querySelectorAll('.option-card').forEach(card => {
            card.setAttribute('tabindex', '0');
            card.addEventListener('keydown', (e) => {
                if (e.key === 'Enter' || e.key === ' ') {
                    e.preventDefault();
                    card.click();
                }
            });
        });
    </script>
</body>
</html>
